  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV='dev' nodemon server.js",
    "start:prod": "NODE_ENV='prod' node server.js",
    "seed": "mongoimport -d paynoattn -c posts --jsonArray --file app/seeds/post_seeds.json --upsert --drop"
