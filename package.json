  "bugs": {
    "url": "https://github.com/paynoattn/paynoattn-api/issues"
  },
  "homepage": "https://github.com/paynoattn/paynoattn-api#readme",
  "dependencies": {
    "JSONStream": "^1.3.1",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "mongoose": "^4.11.7"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
