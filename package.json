    "cors": "^2.8.4",
    "express": "^4.15.4",
    "method-override": "^2.3.9",
    "mongoose": "^4.11.7",
    "morgan": "^1.8.2"
  },
  "devDependencies": {
    "chai": "^4.1.1",
